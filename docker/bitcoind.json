{
  "name": "bitcoind",
  "repo": "ghcr.io/anym001/bitcoind:latest",
  "registry": "https://ghcr.io/",
  "network": "bridge",
  "custom_ip": null,
  "default_shell": "sh",
  "privileged": false,
  "extra_parameters": null,
  "post_parameters": null,
  "web_ui_url": "",
  "icon": "https://raw.githubusercontent.com/anym001/unraid-docker-templates/main/anym001/images/bitcoin.png",
  "project": "https://github.com/bitcoin/bitcoin",
  "support": "",
  "description": "",
  "paths": [
    {
      "name": "Volume 1",
      "host": "/mnt/cache/appdata/bitcoin/bitcoind",
      "container": "/home/bitcoin/.bitcoin",
      "mode": "rw",
      "description": "Volume for bitcoind data",
      "required": true
    }
  ],
  "ports": [
    {
      "name": "Host Port 1",
      "host": "8332",
      "container": "8332",
      "protocol": "tcp",
      "description": "External port 8222 mapped to container port 8222",
      "required": false,
      "mask": false
    },
    {
      "name": "Host Port 2",
      "host": "8333",
      "container": "8333",
      "protocol": "tcp",
      "description": "External port 8333 mapped to container port 8333",
      "required": false,
      "mask": false
    }
  ],
  "variables": [
    {
      "name": "PUID",
      "key": "PUID",
      "value": null,
      "description": "",
      "required": false,
      "mask": false
    },
    {
      "name": "GUID",
      "key": "GUID",
      "value": null,
      "description": "",
      "required": false,
      "mask": false
    },
    {
      "name": "UMASK",
      "key": "UMASK",
      "value": "002",
      "description": "",
      "required": false,
      "mask": false
    }
  ],
  "devices": [],
  "labels": []
}
