{
  "name": "teddycloud",
  "repo": "ghcr.io/toniebox-reverse-engineering/teddycloud:latest",
  "registry": "https://github.com/toniebox-reverse-engineering/teddycloud/pkgs/container/teddycloud",
  "network": "bridge",
  "custom_ip": null,
  "default_shell": "sh",
  "privileged": false,
  "extra_parameters": null,
  "post_parameters": null,
  "web_ui_url": "http://[IP]:[PORT:8080]/",
  "icon": "https://raw.githubusercontent.com/selfhosters/unRAID-CA-templates/master/templates/img/teddycloud.png",
  "project": "https://github.com/toniebox-reverse-engineering/teddycloud",
  "support": "",
  "description": "teddyCloud is an alternative server for your Toniebox, allowing you to host the cloud services locally. This gives you control over which data is sent to the original manufacturerâ€™s cloud and allows you to host your own figurine audio files on, for example, your NAS or any other server.",
  "paths": [
    {
      "name": "content",
      "host": "/mnt/cache/appdata/teddycloud/data/content",
      "container": "/teddycloud/data/content",
      "mode": "rw",
      "description": "",
      "required": false
    },
    {
      "name": "config",
      "host": "/mnt/cache/appdata/teddycloud/config",
      "container": "/teddycloud/config",
      "mode": "rw",
      "description": "",
      "required": true
    },
    {
      "name": "certs",
      "host": "/mnt/cache/appdata/teddycloud/certs",
      "container": "/teddycloud/certs",
      "mode": "rw",
      "description": "Place your certs here after grabbing them off your device. (Yes you need to do this)",
      "required": true
    },
    {
      "name": "library",
      "host": "/mnt/cache/appdata/teddycloud/data/library",
      "container": "/teddycloud/data/library",
      "mode": "rw",
      "description": "",
      "required": false
    },
    {
      "name": "firmware",
      "host": "/mnt/cache/appdata/teddycloud/data/firmware",
      "container": "/teddycloud/data/firmware",
      "mode": "rw",
      "description": "",
      "required": true
    }
  ],
  "ports": [
    {
      "name": "WebGUI",
      "host": "8080",
      "container": "80",
      "protocol": "tcp",
      "description": "This an optional port for the webui of teddycloud",
      "required": false,
      "mask": false
    },
    {
      "name": "SSL-Certs",
      "host": "8443",
      "container": "443",
      "protocol": "tcp",
      "description": "Required PORT to get certificates",
      "required": true,
      "mask": false
    }
  ],
  "variables": [],
  "devices": [],
  "labels": []
}
